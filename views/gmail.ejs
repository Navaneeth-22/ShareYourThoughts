<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <!-- <link rel=”stylesheet”
      href=”https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css”
      /> -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
      crossorigin="anonymous"
    />
    <link
      rel="stylesheet"
      type="text/css"
      href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.14.0-beta3/css/bootstrap-select.min.css"
    />

    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.14.0-beta3/js/bootstrap-select.min.js"></script>
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.6.1/css/bootstrap.min.css"
    />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.6.1/js/bootstrap.bundle.min.js"></script>

    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
      crossorigin="anonymous"
    ></script>
    <link
      href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet"
    />
    <script
      src="https://cdn.socket.io/4.4.1/socket.io.min.js"
      integrity="sha384-fKnu0iswBIqkjxrhQCTZ7qlLHOFEgNkRmK2vaO/LbTZSXdJfAu6ewRBdwHPhBo/H"
      crossorigin="anonymous"
    ></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-cookie/1.4.1/jquery.cookie.js"></script>
    <link href="/css/gmail.css" rel="stylesheet" type="text/css" />

    <title>Democracy</title>
  </head>
  <body style="display: none">
    <div class="header">
      <div class="header_left">
        <span class="material-icons"> menu </span>
        <img src="/images/democracy logo.png" />
      </div>

      <div class="header_right">
        <span class="material-icons profile"> account_circle </span>
      </div>
    </div>
    <!-- Mainbody -->
    <div class="mailbox">
      <!-- sidebar -->
      <div class="sidenav">
        <!-- <button class="composeBut"> -->
        <button
          type="button"
          class="btn btn-primary composeBut"
          data-bs-toggle="modal"
          data-bs-target="#myModal"
        >
          <span class="material-icons"> add </span>
          Compose
        </button>
        <div class="modal" id="myModal" style="display: none">
          <div class="modal-dialog">
            <div class="modal-content">
              <div class="modal-header">
                <h5 class="modal-title">Enter Complaint</h5>
                <button
                  type="button"
                  class="btn-close"
                  data-bs-dismiss="modal"
                ></button>
              </div>
              <div class="modal-body">
                <form>
                  <div class="mb-3">
                    <label class="form-label required">Type:</label>
                    <div class="multi_select_box">
                      <select class="multi_select" id="deptSelec">
                        <option value="#">Select</option>
                        <option value="Main" class="maindept">Main</option>
                        <option value="Road department" class="roaddept">
                          Road department
                        </option>
                        <option
                          value="Water resource department"
                          class="waterdept"
                        >
                          Water resource department
                        </option>
                        <option value="Electricity department" class="elecdept">
                          Electricity department
                        </option>
                      </select>
                    </div>
                  </div>
                  <div class="mb-3">
                    <label class="form-label required">Heading</label>
                    <textarea class="form-control" id="headingId"></textarea>
                  </div>
                  <div class="mb-3">
                    <label class="form-label required">Complaint</label>
                    <textarea class="form-control" id="complaintId"></textarea>
                  </div>
                  <div class="mb-3">
                    <label class="form-label required">Address</label>
                    <textarea class="form-control" id="addressId"></textarea>
                  </div>
                </form>
              </div>
              <div class="modal-footer">
                <button type="submit" class="btn btn-primary" id="complaintBut">
                  Submit
                </button>
                <button type="submit" class="btn btn-danger">Cancel</button>
              </div>
            </div>
          </div>
        </div>
        <button
          type="button"
          class="btn btn-primary mailBut"
          data-bs-toggle="modal"
          data-bs-target="#myModal2"
          style="display: none"
        >
          <span class="material-icons"> add </span>
          MailBut
        </button>
        <div class="modal" id="myModal2" style="display: none">
          <div class="modal-dialog">
            <div class="modal-content">
              <div class="modal-header">
                <h5 class="modal-title">Complaint</h5>
                <button
                  type="button"
                  class="btn-close"
                  data-bs-dismiss="modal"
                ></button>
              </div>
              <div class="modal-body">
                <form>
                  <div class="mb-3">
                    <label class="form-label required">Type:</label>
                    <div class="multi_select_box complaintSelect">
                      <select class="multi_select" id="deptartment">
                        <option value="#">Select</option>
                        <option value="Main" class="maindept">Main</option>
                        <option value="Road department" class="roaddept">
                          Road department
                        </option>
                        <option
                          value="Water resource department"
                          class="waterdept"
                        >
                          Water resource department
                        </option>
                        <option value="Electricity department" class="elecdept">
                          Electricity department
                        </option>
                      </select>
                    </div>
                  </div>
                  <div class="mb-3">
                    <label class="form-label required">Heading</label>
                    <textarea class="form-control" id="header"></textarea>
                  </div>
                  <div class="mb-3">
                    <label class="form-label required">Complaint</label>
                    <textarea class="form-control" id="complaint"></textarea>
                  </div>
                  <div class="mb-3">
                    <label class="form-label required">Address</label>
                    <textarea class="form-control" id="address"></textarea>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
        <div class="sidebarOption sidebarOption_active" id="inbox">
          <span class="material-icons"> inbox </span>
          <h3>Inbox</h3>
        </div>
        <div class="sidebarOption" id="sentByHim">
          <span class="material-icons"> near_me </span>
          <h3>Sent</h3>
        </div>
        <div class="sidebarOption" id="starredByHim">
          <span class="material-icons"> star </span>
          <h3>Starred</h3>
        </div>
      </div>
      <!-- sidebar ends -->
      <div class="emailList">
        <!-- settinglist -->
        <div class="emailList_settings">
          <div class="emailList_settingsLeft">
            <input type="checkbox" />
            <span class="material-icons"> arrow_drop_down </span>
          </div>
          <div class="emailList_settingsRight">
            <span class="material-icons"> chevron_left </span>
            <span class="material-icons"> chevron_right </span>
          </div>
        </div>
        <!-- settinglist ends -->
        <!-- section starts -->
        <div class="emailList_sections">
          <div class="section section_selected mainSection">
            <span class="material-icons"> inbox </span>
            <h4>Primary</h4>
          </div>
          <div class="section roadSection">
            <span class="material-icons"> add_road </span>
            <h4>Road</h4>
          </div>
          <div class="section electricitySection">
            <span class="material-icons"> bolt </span>
            <h4>Electricity</h4>
          </div>
          <div class="section waterSection">
            <span class="material-icons"> water </span>
            <h4>Water</h4>
          </div>
        </div>
        <!-- section ends -->
        <!-- email list row starts -->
        <div>
          <div class="emailList_list">
            <!-- email row starts -->
            <div class="emailRow" style="display: none">
              <div class="emailRow_options">
                <input type="checkbox" />
                <span class="material-icons starSymbol"> star_border </span>
                <span class="material-icons"> label_important </span>
              </div>
              <h3 class="emailRow_title">Youtube</h3>
              <div class="emaiRow_message">
                <h4>You got a new subscriber</h4>
              </div>
              <p class="emailRow_time">10pm</p>
            </div>
            <!-- email  row ends -->
          </div>
        </div>
        <!-- email list row ends -->
      </div>
    </div>
    <!-- mainbody ends -->

    <script src="/js/clientsocket.js"></script>
    <script src="/js/mailPage.js"></script>
  </body>
</html>
